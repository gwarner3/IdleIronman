@model IdleIronman.ViewModels.ParticipantChartViewModel

@{
    Layout = null;
}

@{
    var totalDistanceChart = new Chart(400, 100, ChartTheme.Blue)
        .AddTitle("Total distance covered")
        //.AddLegend()
        .AddSeries(
        name: "Distance covered",
        chartType: "bar",
        xValue: new[] { "1 Distance" },
        yValues: new[] { @Model.biked.ToString() }).Write();

}
@DateTime.Today

